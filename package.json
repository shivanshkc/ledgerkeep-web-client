{
  "name": "ledgerkeep-web-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "pretty:check": "prettier --check . '!.angular/**' '!dist/**'",
    "pretty": "prettier --write . '!.angular/**' '!dist/**'",
    "lint:check": "eslint .",
    "lint": "eslint --fix .",
    "build-image": "docker build -t ledgerkeep-web-client:latest .",
    "build-container": "docker run -d -p 4200:8080 --name ledgerkeep-web-client -v $PWD/src/assets/conf.json:/project/dist/assets/conf.json ledgerkeep-web-client:latest"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "^13.2.3",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "~13.2.0",
    "@angular/material": "^13.2.3",
    "@angular/material-moment-adapter": "^13.2.4",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@swimlane/ngx-charts": "^20.1.0",
    "deepmerge": "^4.2.2",
    "moment": "^2.29.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular/cli": "~13.2.3",
